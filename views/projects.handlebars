<div class="panel-heading">
    <h2 class="panel-title">My Projects</h2>
</div>
<div class="panel-body">
        {{#each projects}}
        <div class="col-xs-6">
            <div class="work">
                <a href="{{repository}}">
                    <img src="{{image}}" alt="{{name}}">
                </a>

                <h3>{{name}}</h3>
            </div>
        </div>
        {{/each}}
</div>